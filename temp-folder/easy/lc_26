class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int k=0;
        for(int i=0;i<nums.size();){
            if(i==nums.size()-1){
                k++;i++;
            }
            else if(nums[i]!=nums[i+1]){
                k++;
                i++;
            }
            else{
                nums.erase(nums.begin()+i);
            }
        }
        return k;
    }
};
