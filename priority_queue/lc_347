class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
        map <int,int> cnt;
        priority_queue<pair<int,int>> pq;
        for(auto i:nums) cnt[i]++;
        for(auto i:cnt){
            pq.push({i.second,i.first});
        }
        vector <int> ans;
        while(k--){
            ans.push_back(pq.top().second);
            pq.pop();
        }
        return ans;
    }
};
