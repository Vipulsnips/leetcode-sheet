class Solution {
public:
    vector<vector<int>> subsets(vector<int>& nums) {
        vector <vector<int>> v;
        vector<int> first={};
        v.push_back(first);
        for(int i=0;i<nums.size();i++){
            int m=v.size();
            for(int j=0;j<m;j++){
                vector <int> temp= v[j];
                temp.push_back(nums[i]);
                v.push_back(temp);
            }
        }
        return v;
    }
};
