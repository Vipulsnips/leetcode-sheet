class Solution {
public:
    vector<vector<int>> permuteUnique(vector<int>& nums) {
        vector<vector< int>> vp;
        sort(nums.begin(),nums.end());
        set<vector <int> > s; vp.push_back(nums);
        s.insert(nums);
        while(next_permutation(nums.begin(),nums.end())){
            if(s.count(nums)==0){
                s.insert(nums);
                vp.push_back(nums);
            }
        }
        return vp;
    }
};
