class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        map<char,int> mp;
        int st=0,e=0,v=0,ans=0,n=s.size();
        while(e<n){
            mp[s[e]]++;
            v++;
            while(mp[s[e]]>1 && st<=e ){
                mp[s[st]]--;
                st++;  
                v--;
            }
            e++;
            ans=max(ans,v);
        }
        return ans;
    }
};
